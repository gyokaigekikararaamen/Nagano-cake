<h2>配送先登録/一覧</h2>
<%= form_with(model:[@address], local: true) do |f| %>
  <div class="field">
    <%= f.label :postal_code,"郵便番号(ハイフンなし)" %>
    <%= f.text_field :postal_code, autocomplete: "postal_code" %>
  </div>

  <div class="field">
    <%= f.label :address,"住所" %>
    <%= f.text_field :address, autocomplete: "address" %>
  </div>

  <div class="field">
    <%= f.label :name,"宛名" %>
    <%= f.text_field :name, autocomplete: "name" %>
  </div>

  <%= f.submit "送信する" %>
<% end %>